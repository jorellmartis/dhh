{
  "kind": "collectionType",
  "collectionName": "pages_properties",
  "info": {
    "singularName": "pages-property",
    "pluralName": "pages-properties",
    "displayName": "Pages - Property",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "imgThumb": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Location": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::list-location.list-location",
      "inversedBy": "Property"
    },
    "price": {
      "type": "string"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "webiste-blocks.image-gallery",
        "webiste-blocks.property-description",
        "webiste-blocks.facilities",
        "webiste-blocks.extra-facility",
        "webiste-blocks.property-rules",
        "webiste-blocks.district-and-area"
      ]
    },
    "SEO": {
      "type": "component",
      "repeatable": false,
      "component": "global-blocks.seo"
    },
    "list_bedroom": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::list-bedroom.list-bedroom",
      "inversedBy": "pages_properties"
    }
  }
}
